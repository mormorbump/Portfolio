    <div class="col-md-6">
      <% @gyms.each do |gym| %>
      <div class="gymbox">
        <div class="gymtitle">
          <%= link_to (gym.name), gym_path(gym) %>
        </div>
        <div class="gympicture">
          <%= image_tag gym.picture.first.url  %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
