<%= form_with(model: @gym, local: true) do |f| %>

  <%= f.label :ジム名 %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :口コミ %>
  <%= f.text_field :content, class: 'form-control' %>

  <%= f.label :写真 %>
  <%= f.file_field :picture %>

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
